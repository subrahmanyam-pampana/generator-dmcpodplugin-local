{
  "welcomeFile": "/<%= name %>/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/<%= name %>/(.*)$",
      "target": "$1",
      "localDir": "dist",
      "cacheControl": "public, max-age=3600, must-revalidate"
    }
  ]
}