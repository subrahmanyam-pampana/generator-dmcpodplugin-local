{
  "welcomeFile": "/<%= name %>/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/<%= name %>/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "cacheControl": "public, max-age=43200, must-revalidate"
    },
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "destination":"SAP_DMC_DEFAULT_SERVICE_KEY"
    }
  ]
}